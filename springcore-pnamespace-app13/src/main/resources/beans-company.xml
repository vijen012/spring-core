<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean name="company" class="com.springcore.domain.Company"
		p:companyName="Google" p:yearOfIncorporation="1997" p:address-ref="address" />
	<bean id="address" class="com.springcore.domain.Address" p:street="Staines Road"
		p:city="London" p:zipCode="TW33GE" />
</beans>
